{
  "transform": {
    "^.+index\\.jsx?$": "@swissquote/crafty-preset-jest/src/esm-transformer"
  },
  "collectCoverage": true,
  "coveragePathIgnorePatterns": [
  ],
  "collectCoverageFrom": [
    "<rootDir>/src/main/**/*.js"
  ],
  "testMatch": [
    "<rootDir>/src/test/js/*.js"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "<rootDir>/src/test/*/rules"
  ],
  "moduleFileExtensions": [
    "js",
    "jsx",
    "json",
    "node"
  ]
}
